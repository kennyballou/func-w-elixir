iex> %{}
%{}
iex> %{"key" => "value"}
%{"key" => "value"}
iex> %{a: 2, b: 1}
%{a: 2, b: 1}
iex> [{:a, 2}, {:b, 1}]
[a: 2, b: 1]
iex> [a: 2, b: 1]
[a: 2, b: 1]

iex> my_map = %{a: 2, b: 1}
%{a: 2, b: 1}
iex> my_map[:a]
2
iex> Map.get(my_map, :a)
2
iex> my_map.a
2
iex> Map.put(my_map, :c, 4)
%{a: 2, b: 1, c: 4}
iex> Map.put(my_map, :a, 42)
%{a: 42, b: 1}
